    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core componets
            Layout.init(); // init layout

            // Menu
            var $menu = $("ul.page-sidebar-menu li.active");
            var $menuItems = $menu.parents("li");
            $menuItems.addClass("open");
            $menu.parents("ul.sub-menu").css("display", "block");

            var $arr = $menuItems.get().reverse();
            $arr.push($menu);

            var $code = '';
            $.each($arr, function (i, e) {
                var title = $(e).find('.title').eq(0).text();
                var icon = $(e).find('.icon-header').eq(0).attr('class');

                $code += '<li>';
                $code += '<i class="' + icon + '"></i>';
                $code += '<a href="javascript:void(0);">' + title + '</a>';

                if (i < $arr.length - 1)
                    $code += '<i class="fa fa-angle-right"></i>';

                $code += '</li>';
            });
            $('.page-breadcrumb').html($code);
        });

        // grid template
        page.Templates.DataGridBar_NavigationButton = '<a class="btn btn-success DataGridBar_RecordCount" href="#"></a>';
        page.Templates.DataGridBar_Main = '#Template_DataGridBar';
